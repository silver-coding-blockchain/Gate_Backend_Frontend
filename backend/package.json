{
  "name": "gated-backend",
  "version": "0.0.1",
  "description": "",
  "author": "Allen Hartwig",
  "private": false,
  "license": "MIT",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:strict": "nest build -p ./tsconfig.strict.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev:init": "mkdir -p ./docker/docker-volumes/data/pg",
    "dev:up": "docker compose -f ./docker/docker-compose.yaml up -d",
    "dev:down": "docker compose -f ./docker/docker-compose.yaml down",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --watch --debug",
    "start:prod": "node dist/main",
    "hmr:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "hmr:debug": "DEBUG=true npm run start:dev",
    "db:migrate": "mikro-orm migration:up",
    "db:seed": "ts-node -r dotenv/config ./schema/seed",
    "lint": "eslint \"{src,apps,test}/**/*.ts\" --fix",
    "lint:strict": "eslint -c ./.eslintrc.strict.js \"{src,apps,test}/**/*.ts\" --fix",
    "lint:ci": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --forceExit --config ./test/jest.config.json",
    "test:e2e:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest.config.json",
    "test:ci": "jest --forceExit",
    "prettier:ci": "prettier --check ./src",
    "prepare": "husky install"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.17.0",
    "@google-cloud/kms": "^2.10.0",
    "@google-cloud/opentelemetry-cloud-monitoring-exporter": "^0.14.0",
    "@google-cloud/opentelemetry-cloud-trace-exporter": "^1.0.0",
    "@google-cloud/pubsub": "^2.18.1",
    "@google-cloud/storage": "^5.18.1",
    "@mikro-orm/cli": "^4.5.9",
    "@mikro-orm/core": "^4.5.9",
    "@mikro-orm/migrations": "^4.5.9",
    "@mikro-orm/nestjs": "^4.3.1",
    "@mikro-orm/postgresql": "^4.5.9",
    "@nestjs-steroids/async-context": "^2.0.0",
    "@nestjs/bull": "^0.4.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.0.0",
    "@nestjs/cqrs": "^8.0.0",
    "@nestjs/graphql": "^9.0.6",
    "@nestjs/microservices": "^8.4.7",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/websockets": "^8.4.7",
    "@opentelemetry/api": "^1.0.3",
    "@opentelemetry/api-metrics": "^0.27.0",
    "@opentelemetry/auto-instrumentations-node": "^0.26.2",
    "@opentelemetry/core": "^1.4.0",
    "@opentelemetry/exporter-zipkin": "^1.0.0",
    "@opentelemetry/instrumentation": "^0.26.0",
    "@opentelemetry/resources": "^1.4.0",
    "@opentelemetry/sdk-metrics-base": "^0.27.0",
    "@opentelemetry/sdk-trace-base": "^1.4.0",
    "@opentelemetry/sdk-trace-node": "^1.4.0",
    "@opentelemetry/semantic-conventions": "^1.4.0",
    "@sendgrid/mail": "^7.6.0",
    "address-rfc2822": "^2.1.0",
    "analytics-node": "^6.0.0",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "dataloader": "^2.0.0",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "fast-crc32c": "^2.0.0",
    "firebase-admin": "^10.0.0",
    "gaxios": "^5.0.2",
    "google-auth-library": "^8.7.0",
    "google-gax": "^2.25.3",
    "googleapis": "^111.0.0",
    "googleapis-common": "^6.0.4",
    "graphql": "^15.8.0",
    "jsonwebtoken": "^8.5.1",
    "lightship": "^6.8.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.34",
    "mimetext": "^2.0.7",
    "node-ical": "^0.15.1",
    "pg": "^8.7.1",
    "pino": "^7.9.2",
    "pino-http": "^6.0.0",
    "pino-stackdriver": "^3.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.5",
    "source-map-support": "^0.5.21",
    "stripe": "^8.191.0",
    "ts-morph": "^12.2.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bluebird": "^3.5.37",
    "@types/bull": "^3.15.5",
    "@types/cache-manager": "^3.4.2",
    "@types/cache-manager-redis-store": "^2.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.1",
    "@types/fast-crc32c": "^2.0.0",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mime-types": "^2.1.1",
    "@types/mimetext": "^2.0.1",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.2",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "deepmerge": "^4.2.2",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^5.5.3",
    "husky": "^8.0.1",
    "jest": "^27.0.6",
    "jest-node-exports-resolver": "^1.1.2",
    "lint-staged": "^13.0.3",
    "nock": "^13.2.1",
    "pino-pretty": "^7.6.0",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.0.11",
    "supertest": "^6.1.3",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.64.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "resolver": "jest-node-exports-resolver",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "^@app/interfaces(|/.*)$": "<rootDir>/src/interfaces/$1",
      "^@app/errors(|/.*)$": "<rootDir>/src/errors/$1",
      "^@app/events(|/.*)$": "<rootDir>/src/events/$1",
      "^@app/modules(|/.*)$": "<rootDir>/src/modules/$1"
    },
    "setupFiles": [
      "dotenv/config"
    ]
  },
  "lint-staged": {
    "*.{ts,js,json,yaml,yml}": "prettier --write --ignore-unknown"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
